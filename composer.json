{
  "name": "proklung/wp-cycle-orm",
  "description": "Spiral Cycle ORM integration for Wordpress",
    "keywords": [
        "wordpress",
        "wordpress-symfony"
    ],

  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Gavrilov Fedy",
      "email": "fedor.gavrilov.75@mail.ru"
    }
  ],

  "support": {
    "issues": "https://github.com/ProklUng/wp.cycle.orm/issues",
    "source": "https://github.com/ProklUng/wp.cycle.orm"
  },
  "autoload": {
    "psr-4": {
      "Prokl\\WpCycleOrmBundle\\": "src",
      "Prokl\\WpCycleOrmBundle\\Tests\\": "tests"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/proklung/wordpress-test-build"
    }
  ],
  "require": {
    "php": ">=7.3 | ~8",
    "symfony/dependency-injection": "^4.0 || ^5.0",
    "symfony/http-kernel": "^4.0 || ^5.0",
    "symfony/config": "~4|~5",
    "symfony/framework-bundle": "^4.0 || ^5.0",
    "proklung/base-exception": "^1.0",
    "cycle/orm": "^1.4",
    "cycle/annotated": "^2.3",
    "cycle/schema-builder": "^1.2",
    "cycle/proxy-factory": "^1.3",
    "spiral/tokenizer": "^2.7"
  },
  "require-dev": {
    "proklung/wordpress-test-build": "^1.1"
  }
}
